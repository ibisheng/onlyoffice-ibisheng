    "deploy": {
        "copy": {
            "api": {
                "expand": true,
                "cwd": "../../PowerPoint/",
                "src": [
                    "../Common/apiBase.js",
                    "api.js",
                    "apiCommon.js",
                    "apiDefines.js"
                ],
                "dest": "<%= pkg.menu_path %>/PowerPoint/"
            },
            "sdk": {
                "expand": true,
                "cwd": "../../PowerPoint/",
                "src": [
                    "sdk-all.js",
                    "themes/theme*/**"
                ],
                "dest": "<%= pkg.menu_path %>/PowerPoint/"
            },
            "doc": {
                "expand": true,
                "cwd": "../../PowerPoint/",
                "src": "document/**",
                "dest": "<%= pkg.menu_path %>/PowerPoint/"
            },
            "log": {
                "expand": true,
                "cwd": "../../PowerPoint/",
                "flatten": true,
                "src": [
                    "sdk-all.js.map",
                    "Build/Log/property.map",
                    "Build/Log/variable.map"
                ],
                "dest": "<%= pkg.menu_path %>/PowerPoint/"
            },
            "sample": {
                "expand": true,
                "cwd": "../../PowerPoint/",
                "src": "index.html.deploy",
                "dest": "<%= pkg.menu_path %>/PowerPoint/"
            }
        },
        "store_log": {
            "dst": "../../PowerPoint/Build/Log"
        }
